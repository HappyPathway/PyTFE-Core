{
  "data": {
    "type":"vars",
    "attributes": {
      "key":"{{ variable.key }}",
      {% if not variable.hcl %}
      "value":"{{ variable.value }}",
      {% else %}
      "value": {{ variable.value }},
      {% endif %}
      "category":"{{ variable.category }}",
      "hcl":{{ variable.hcl|lower }},
      "sensitive":{{ variable.sensitive|lower }}
    },
    "relationships": {
      "workspace": {
        "data": {
          "id":"{{ variable.workspace_id }}",
          "type":"workspaces"
        }
      }
    }
  }
}